<% if(paginate.hasPreviousPages || paginate.hasNextPages(pageCount)){%>
    <div class="row mt-3">
        <div class="col-sm-12 col-md-5">
            <!-- <div class="dataTables_info" id="bootstrap-data-table_info" role="status" aria-live="polite">Showing 1 to 10 of 57 entries</div> -->
        </div>
        <div class="col-sm-12 col-md-7">
            <div class="dataTables_paginate paging_simple_numbers" id="bootstrap-data-table_paginate">
                <ul class="pagination">
                    <% if(paginate.hasPreviousPages){%>
                    <li class="paginate_button page-item previous" id="bootstrap-data-table_previous">
                        <a href="<%=paginate.href(true)%>" aria-controls="bootstrap-data-table" class="page-link">
                            Previous
                        </a>
                    </li>
                    <%}
                        if(pages){
                            pages.forEach((page)=>{
                        %>
                    <li class="paginate_button page-item <% if(activePage == page.number){%>active<%}%>">
                        <a href="<%= page.url %>" aria-controls="bootstrap-data-table" data-dt-idx="<%=page.number%>" tabindex="0" class="page-link">
                            <%=page.number%>
                        </a>
                    </li>
                    <%});
                        }%>
                    <% if(paginate.hasNextPages(pageCount)){ %>
                    <li class="paginate_button page-item next" id="bootstrap-data-table_next">
                        <a href="<%=paginate.href()%>" aria-controls="bootstrap-data-table" class="page-link">Next</a>
                    </li>
                    <% } %>
                </ul>
            </div>
        </div>
    </div>
<%}%>

